---
const { source, className } = Astro.props;

import {
  FaPhone,
  FaGlobe,
  FaLocationPin,
  FaBehance,
  FaBitbucket,
  FaCodepen,
  FaDiscord,
  FaDribbble,
  FaFacebook,
  FaFoursquare,
  FaGithub,
  FaGitlab,
  FaInstagram,
  FaLinkedin,
  FaMastodon,
  FaMedium,
  FaPinterest,
  FaReddit,
  FaRss,
  FaSkype,
  FaSlack,
  FaSnapchat,
  FaSoundcloud,
  FaTiktok,
  FaTumblr,
  FaXTwitter,
  FaVimeo,
  FaVk,
  FaWhatsapp,
  FaYoutube,
  FaEnvelope,
} from "react-icons/fa6";

const {
  facebook,
  twitter,
  mastodon,
  instagram,
  youtube,
  linkedin,
  github,
  gitlab,
  discord,
  slack,
  medium,
  codepen,
  bitbucket,
  dribbble,
  behance,
  pinterest,
  soundcloud,
  tumblr,
  reddit,
  vk,
  whatsapp,
  snapchat,
  vimeo,
  tiktok,
  foursquare,
  rss,
  email,
  phone,
  address,
  skype,
  website,
} = source;
---

<ul class={className}>
  {
    facebook && (
      <li class="inline-block">
        <a
          aria-label="facebook"
          href={facebook}
          target="_blank"
          rel="noopener noreferrer nofollow"
        >
          <FaFacebook />
        </a>
      </li>
    )
  }
